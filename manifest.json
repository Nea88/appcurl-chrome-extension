{
    "manifest_version": 2,
    "name": "Appcurl",
    "description": "Appcurl search engine",
    "version": "1",
    "app": {
        "urls": [
            "*://appcurl.com",
            "*://www.appcurl.com"
        ],
        "launch": {
            "web_url": "http://appcurl.com"
        }
    },
    "icons": {
        "128": "128x128.png",
        "48": "48x48.png",
        "32": "32x32.png",
        "16": "16x16.png"
    }
}